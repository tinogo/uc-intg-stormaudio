{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "changelog-sections": [
    {
      "type": "feat",
      "section": "Features",
      "hidden": false
    },
    {
      "type": "fix",
      "section": "Bug Fixes",
      "hidden": false
    },
    {
      "type": "docs",
      "section": "Documentation",
      "hidden": false
    },
    {
      "type": "chore",
      "section": "Miscellaneous",
      "hidden": false
    },
    {
      "type": "refactor",
      "section": "Miscellaneous",
      "hidden": false
    },
    {
      "type": "revert",
      "section": "Reverts",
      "hidden": false
    }
  ],
  "include-component-in-tag": false,
  "packages": {
    ".": {
      "release-type": "python",
      "extra-files": [
        "driver.json",
         {
          "path": "uv.lock",
          "type": "toml",
          "jsonpath": "$.package[?(@.name.value=='uc-intg-stormaudio')].version"
        }
      ]
    }
  }
}